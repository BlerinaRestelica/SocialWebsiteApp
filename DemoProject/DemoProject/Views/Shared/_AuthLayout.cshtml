<!DOCTYPE html>
<html>
<head>
    @RenderSection("head", required: false)
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>SocialV</title>
    <link rel="shortcut icon" href=@Url.Content("../Content/assets/images/favicon.ico") />
    <link rel="stylesheet" href=@Url.Content("../Content/assets/css/libs.min.css")>
    <link rel="stylesheet" href=@Url.Content("../Content/assets/css/socialv.css?v=4.0.0")>
    <link rel="stylesheet" href=@Url.Content("../Content/assets/vendor/fortawesome/fontawesome-free/css/all.min.css")>
    <link rel="stylesheet" href=@Url.Content("../Content/assets/vendor/remixicon/fonts/remixicon.css")>
    <link rel="stylesheet" href=@Url.Content("../Content/assets/vendor/vanillajs-datepicker/dist/css/datepicker.min.css")>
    <link rel="stylesheet" href=@Url.Content("../Content/assets/vendor/font-awesome-line-awesome/css/all.min.css")>
    <link rel="stylesheet" href=@Url.Content("../Content/assets/vendor/line-awesome/dist/line-awesome/css/line-awesome.min.css")>
</head>
<body>
    <!-- loader Start -->
    <div id="loading">
        <div id="loading-center">
        </div>
    </div>
    <!-- loader END -->
    <div class="wrapper">
        <section class="sign-in-page">
            <div id="container-inside">
                <div id="circle-small"></div>
                <div id="circle-medium"></div>
                <div id="circle-large"></div>
                <div id="circle-xlarge"></div>
                <div id="circle-xxlarge"></div>
            </div>
            <div class="container p-0">
                <div class="row no-gutters">
                    <div class="col-md-6 text-center pt-5">
                        <div class="sign-in-detail text-white">
                            <a class="sign-in-logo mb-5" href="#"><img src="../Content/assets/images/logo-full.png" class="img-fluid" alt="logo"></a>
                            <div class="sign-slider overflow-hidden ">
                                <ul class="swiper-wrapper list-inline m-0 p-0 ">
                                    <li class="swiper-slide">
                                        <img src="../Content/assets/images/login/1.png" class="img-fluid mb-4" alt="logo">
                                        <h4 class="mb-1 text-white">Find new friends</h4>
                                        <p>It is a long established fact that a reader will be distracted by the readable content.</p>
                                    </li>
                                    <li class="swiper-slide">
                                        <img src="../Content/assets/images/login/2.png" class="img-fluid mb-4" alt="logo">
                                        <h4 class="mb-1 text-white">Connect with the world</h4>
                                        <p>It is a long established fact that a reader will be distracted by the readable content.</p>
                                    </li>
                                    <li class="swiper-slide">
                                        <img src="../Content/assets/images/login/3.png" class="img-fluid mb-4" alt="logo">
                                        <h4 class="mb-1 text-white">Create new events</h4>
                                        <p>It is a long established fact that a reader will be distracted by the readable content.</p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                    @RenderBody()
                </div>
            </div>
        </section>
    </div>
    <!-- Backend Bundle JavaScript -->
    <script src="../Content/assets/js/libs.min.js"></script>
    <!-- slider JavaScript -->
    <script src="../Content/assets/js/slider.js"></script>
    <!-- masonry JavaScript -->
    <script src="../Content/assets/js/masonry.pkgd.min.js"></script>
    <!-- SweetAlert JavaScript -->
    <script src="../Content/assets/js/enchanter.js"></script>
    <!-- SweetAlert JavaScript -->
    <script src="../Content/assets/js/sweetalert.js"></script>
    <!-- app JavaScript -->
    <script src="../Content/assets/js/charts/weather-chart.js"></script>
    <script src="../Content/assets/js/app.js"></script>
    <script src="../Content/vendor/vanillajs-datepicker/dist/js/datepicker.min.js"></script>
    <script src="../Content/assets/js/lottie.js"></script>
</body>
</html>

